{
    "name": "foodticket/laravel-bird-driver",
    "description": "Bird mail driver for Laravel 9+",
    "keywords": ["PHP", "Bird", "mail driver", "Laravel mail driver"],
    "version": "1.0.0-beta.1",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "spatie/laravel-data": "^3.10",
        "illuminate/mail": "^9.0||^10.0||^11.0",
        "illuminate/notifications": "^9.0||^10.0||^11.0",
        "illuminate/support": "^9.0||^10.0||^11.0",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "illuminate/container": "^9.0||^10.0||^11.0",
        "illuminate/filesystem": "^9.0||^10.0||^11.0",
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.15",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^9",
        "laravel/helpers": "^1.2",
        "vlucas/phpdotenv": "^5.4.1",
        "fakerphp/faker": "^1.23",
        "rector/rector": "*"
    },
    "autoload": {
        "psr-4": {
            "Foodticket\\LaravelBirdDriver\\": "src"
        }
    },
    "authors": [
        {
            "name": "Rik Morskate",
            "email": "rik@foodticket.nl"
        },
        {
            "name": "Dylan G",
            "email": "dylan@foodticket.nl"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Foodticket\\LaravelBirdDriver\\BirdServiceProvider"
            ]
        }
    }
}
